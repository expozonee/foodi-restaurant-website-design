{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "/images/(.*)",
      "use": "@vercel/static",
      "config": { "source": "images/$1" }
    },
    {
      "src": "/css/(.*)",
      "use": "@vercel/static",
      "config": { "source": "css/$1" }
    },
    {
      "src": "/js/(.*)",
      "use": "@vercel/static",
      "config": { "source": "js/$1" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
